    speakerID: Torture_Veronica
    dialogueText: One year ago. November, 1971.
    cameraShotID: FB_OfficeView
    animationTrigger: Yawn
    lineSFX: {fileID: 0}
    displayDuration: 3
  - participantsToSetVisible:
    - FlashbackO_DanteCaller
    speakerID: FlashbackO_DanteCaller
    dialogueText: "Dante... Are you really sure you're writing a paper on that? I
      know the company has been publishing riskier articles as of late, but you don\u2019t
      have to include yourself in that."
    cameraShotID: FB_TalkPhone
    animationTrigger: ListenPhone
    lineSFX: {fileID: 0}
    displayDuration: 3
  - participantsToSetVisible:
    - FlashbackO_Dante
    speakerID: FlashbackO_Dante
    dialogueText: I have t-
    cameraShotID: FB_TalkPhone2
    animationTrigger: TPHesitate
    lineSFX: {fileID: 0}
    displayDuration: 3
  - participantsToSetVisible:
    - FlashbackO_Dante
    speakerID: FlashbackO_Dante
    dialogueText: "I WANT to. I just\u2026 believe these stories need to be told."
    cameraShotID: FB_TalkPhone2
    animationTrigger: TPTalk2
    lineSFX: {fileID: 0}
    displayDuration: 3
  - participantsToSetVisible:
    - FlashbackO_DanteCaller
    speakerID: FlashbackO_DanteCaller
    dialogueText: "Now you\u2019re sounding like those journalists that go missing."
    cameraShotID: FB_TalkPhone3
    animationTrigger: ListenPhone
    lineSFX: {fileID: 0}
    displayDuration: 3
  - participantsToSetVisible:
    - FlashbackO_DanteCaller
    speakerID: FlashbackO_DanteCaller
    dialogueText: "That said, don\u2019t go too far. And prioritize your safety,
      okay?"
    cameraShotID: FB_TalkPhone3
    animationTrigger: ListenPhone
    lineSFX: {fileID: 0}
    displayDuration: 3
  - participantsToSetVisible:
    - FlashbackO_Dante
    speakerID: FlashbackO_Dante
    dialogueText: Okay. Thanks, chief.
    cameraShotID: FB_TalkPhone4
    animationTrigger: EndCall
    lineSFX: {fileID: 0}
    displayDuration: 3
  - participantsToSetVisible:
    - FlashbackO_Dante2
    speakerID: FlashbackO_Dante2
    dialogueText: ...
    cameraShotID: FB_DanteSad2
    animationTrigger: 
    lineSFX: {fileID: 0}
    displayDuration: 3
  - participantsToSetVisible:
    - FlashbackO_Dante2
    speakerID: FlashbackO_Dante2
    dialogueText: "(Mang Ernesto really died\u2026 I thought someone else would write
      his story.. No one did\u2026)\r\n"
    cameraShotID: FB_DanteSad
    animationTrigger: SadWalk
    lineSFX: {fileID: 0}
    displayDuration: 3
  - participantsToSetVisible:
